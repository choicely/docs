{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Choicely Mobile SDK",
  "colors": {
    "primary": "#00D68F",
    "light": "#00FFB3",
    "dark": "#00B377"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Mobile",
        "groups": [
          {
            "group": "Android",
            "pages": [
              "android/quick-start",
              "android/firebase",
              "android/map",
              "android/shop"
            ]
          },
          {
            "group": "Android Advanced",
            "pages": [
              "android-advanced/custom-splash",
              "android-advanced/custom-fragment"
            ]
          },
          {
            "group": "iOS",
            "pages": [
              "ios/quick-start",
              "ios/firebase",
              "ios/map",
              "ios/shop"
            ]
          },
          {
            "group": "iOS Advanced",
            "pages": [
              "ios-advanced/custom-view"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "resources/firebase-project",
              "resources/map-api",
              "resources/store-setup"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Choicely Studio",
          "href": "https://studio.choicely.com",
          "icon": "rocket"
        },
        {
          "anchor": "Website",
          "href": "https://www.choicely.com",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/choicely-logo.png",
    "dark": "/logo/choicely-logo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@choicely.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Choicely Studio",
      "href": "https://studio.choicely.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "facebook": "https://www.facebook.com/Choicelyapp/",
      "instagram": "https://www.instagram.com/choicelyapp/",
      "linkedin": "https://linkedin.com/company/choicely"
    }
  }
}
